{
  "name": "clipboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "tailwindcss -i ./input.css -o ./css/style.css --watch",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch",
    "compile:tailwind": "tailwindcss -i ./input.css -o ./css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:tailwind prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuzerShakir/clipboard.git"
  },
  "keywords": [
    "tailwind"
  ],
  "author": "Juzer Shakir",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JuzerShakir/clipboard/issues"
  },
  "homepage": "https://github.com/JuzerShakir/clipboard#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.3"
  }
}
